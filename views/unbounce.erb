<h1> Unbounce params</h1>
<div>
  <h2> Debug information </h2>
  <p> Referrer: <%= @referrer %> </p>
  <p> IP: <%= @ip %> </p>
  <p> URL: <%= @url %> </p>
</div>
<p>
<%= @params.inspect %>
</p>
<p>
  <% @params.each do |param| %>
    <div class="param">
      <span class="key"> <%= param[0] %> </span>
      <% if param[1].empty? %>
        <span class="value empty"> no value given </span>
      <% end %>
      <span class="value"> <%= param[1] %> </span>
    </div>
  <% end %>
</p>
